-weight: 400;\n    color: rgb(60, 60, 60);\n\n\n    span { \n        font-weight: 700; \n        color: rgb(121, 27, 27); \n    }\n}\n\n\n.form .logo, \n.shoppinglistcontainer .logo {\n    margin: 1rem 0 2rem 0;\n}\n\n.basecontainer .wrapper .logo {\n    margin: 3rem auto;\n    text-align: center;\n}","css/_basecontainer.scss":".basecontainer {\n    width: 100%;\n    min-height: 100vh;\n\n    display: flex;\n    flex-direction: column;\n\n    \n    .app-title-filler {\n        width: 100%;\n        height: 7.5vh;\n    }\n}","css/_form.scss":".form {\n    width: 100%;\n    margin: 5.5vh 0;\n    \n    display: flex;\n    flex-direction: column;\n    \n    padding: 0 5vh 0 5vh;\n\n    animation-name: toggleAwake;\n    animation-duration: .5s;\n\n    \n    .form-group {\n        padding: .65rem;\n    }\n}","css/_shoppinglistcontainer.scss":".shoppinglistcontainer {\n    padding: 2.5rem 1.9rem;\n}","css/_shoppinglistitem.scss":".shoppingitem {\n    width: 100%;\n    height: 4.5rem;\n\n    display: flex;\n    flex-direction: row;\n    margin: 0 0 1rem 0;\n\n    background-color: rgb(240, 240, 240);\n    /* border-bottom: 2px solid rgb(121, 27, 27); */\n    transition: all 300ms ease-in-out;\n\n\n    &-quantity {\n        flex: .7;\n    \n        background-color: rgb(121, 27, 27);\n        display: flex;\n        justify-content: center;\n        align-items: center;\n        transition: all 300ms ease-in-out;\n\n        h2 {\n            color: rgb(221, 221, 221);\n            transition: all 300ms ease-in-out;\n        }\n    }\n\n\n    &-details {\n        flex: 7.5;\n\n        margin-left: 1rem;\n\n        display: flex;\n        flex-direction: column;\n        justify-content: center;\n        align-items: flex-start;\n\n\n        h1 {\n            color: rgb(121, 27, 27);\n            transition: all 300ms ease-in-out;\n        }\n\n\n        p {\n            margin-top: .3rem;\n            font-style: italic;\n            transition: all 300ms ease-in-out;\n        }\n    }\n\n    \n    &-controls {\n        flex: 2.3;\n        display: flex;\n    }\n}\n\n\n.shoppingitem.checked {\n\n    background-color: white;\n    \n\n    .shoppingitem-quantity {\n        background-color: rgb(230, 213, 213);\n        \n        h2 { \n            color: rgb(187, 187, 187);\n        }\n    }\n\n\n    .shoppingitem-details {\n        h1 {\n            color: rgb(187, 187, 187);\n        }\n\n        p {\n            color: rgb(187, 187, 187);\n        }\n    }\n\n    button {\n        background-color:rgb(243, 243, 243);\n        color: rgb(187, 187, 187);\n    }\n}"},"lineCount":null}},"error":null,"hash":"f9d743adf3a08d8fed2f740c833dac1b","cacheData":{"env":{}}}